{
  "name": "node-network-tab",
  "version": "1.1.1",
  "description": "Chrome DevTools-style network inspector for Node.js - intercepts HTTP/HTTPS requests and displays them in an interactive terminal dashboard",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "node-network-tab": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./start": {
      "import": "./dist/start.js",
      "types": "./dist/start.d.ts"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node --import ./dist/start.js test/demo.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "network",
    "http",
    "https",
    "inspector",
    "debugger",
    "devtools",
    "terminal",
    "tui",
    "cli",
    "intercept",
    "monitor",
    "request",
    "response",
    "api",
    "fetch",
    "axios",
    "node",
    "debug",
    "logging",
    "network-inspector",
    "http-inspector",
    "request-logger",
    "developer-tools"
  ],
  "author": "rohit-jsfreaky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit-jsfreaky/node-network-tab.git"
  },
  "bugs": {
    "url": "https://github.com/rohit-jsfreaky/node-network-tab/issues"
  },
  "homepage": "https://github.com/rohit-jsfreaky/node-network-tab#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rohit-jsfreaky"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "ink": "^5.0.1",
    "ink-text-input": "^6.0.0",
    "pretty-ms": "^9.1.0",
    "react": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
